CROSS_COMPILE=
CC = $(TIGCC)/bin/tigcc
ASM = $(TIGCC)/bin/tigcc
AR = $(TIGCC)/bin/tigcc
LINKER = $(TIGCC)/bin/tigcc
PLATFORM = 68000
export PLATFORM
